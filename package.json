{
  "name": "q-dialog",
  "version": "0.1.0",
  "description": "A delicate vue mobile dialog",
  "main": "dist/q-dialog.esm.js",
  "scripts": {
    "check": "yarn upgrade-interactive --latest",
    "dev:demo": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack.demo.js",
    "lint": "eslint . --ext .js,.ts,.tsx,.d.ts && stylelint **/*.scss",
    "build": "yarn build:pack && yarn build:demo",
    "build:demo": "rimraf demoDist && cross-env NODE_ENV=production webpack --config build/webpack.demo.js",
    "build:pack": "node build/rollup.package.js",
    "test": "cross-env NODE_ENV=test karma start tests/karma.config.js",
    "test:single": "cross-env NODE_ENV=test karma start tests/karma.config.js --single-run",
    "test:ci": "NODE_ENV=test jest && codecov",
    "pub": "node scripts/publish",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 1",
    "commit": "git cz"
  },
  "author": {
    "name": "Qymh",
    "email": "415570450@qq.com"
  },
  "keywords": [
    "vue",
    "dialog",
    "components"
  ],
  "engines": {
    "node": ">=8.15.1"
  },
  "types": "types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Qymh/q-dialog.git"
  },
  "license": "MIT",
