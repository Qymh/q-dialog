  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.scss": [
      "stylelint --fix",
      "git add"
    ],
    "*.(t|j)s": [
      "eslint --ext .js,.ts,.d.ts --fix",
      "git add"
    ],
    "*.(t|j)sx": [
      "eslint --ext .tsx --fix",
      "git add"
    ]
  }
